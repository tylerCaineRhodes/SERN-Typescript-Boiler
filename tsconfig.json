{
  "compilerOptions": {
    "target": "es5",                         
    "module": "es2015",                     
    "allowJs": true,                       
    "jsx": "react",                     
    "outDir": "./client/dist",                        
    "noImplicitAny": true,                 
    "moduleResolution": "node",            
    "allowSyntheticDefaultImports": true,  
    "skipLibCheck": true,                  
    "forceConsistentCasingInFileNames": true
  }
}
